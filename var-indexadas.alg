algoritmo "semnome"
var
peoples:vetor[1..8] de Caractere
ages:vetor[1..8] de Inteiro

inicio
peoples[1] <- "Bruno"
peoples[2] <- "Gabriel"
ages[1] <- 16
ages[2] <- 16
escreval("Nome: ", peoples[1], " | Idade: ", ages[1])
escreval("Nome: ", peoples[2], " | Idade: ", ages[2])
fimalgoritmo
